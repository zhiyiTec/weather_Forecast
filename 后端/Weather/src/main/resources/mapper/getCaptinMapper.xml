<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.boot.zhiyi.mapper.GetCaptinMapper">
	<select id="getCaptinCityByName" parameterType="String" resultType="com.boot.zhiyi.model.Weather">
		select * from
		captincity where captinName=#{name} 
	</select>
</mapper>
